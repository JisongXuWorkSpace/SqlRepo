-- 更改業主

select PROP_ID from rpm_proprietor where PROP_CODE='SYNNEX';


SELECT MT.PROCESSCODE,MT.* FROM RPMMT MT 
WHERE 1=1
AND MT.MTNO IN (
'MT66924104',
'MT66925483',
'MT66926183',
'MT66925488',
'MT66926782'
)

SELECT RG.* FROM RPMRG RG ,(
SELECT MT.PROCESSCODE,MT.* FROM RPMMT MT 
WHERE 1=1
AND MT.MTNO IN (
'MT66924104',
'MT66925483',
'MT66926183',
'MT66925488',
'MT66926782'
)
)MTT
WHERE 1=1
AND MTT.REGISTERNO = RG.REGISTERNO



SELECT RG.* FROM RPMRG RG ,(
SELECT MT.PROCESSCODE,MT.* FROM RPMMT MT 
WHERE 1=1
AND MT.MTNO IN (
'MT2D902841',
'MT3A901915',
'MT3C902824',
'MT3A901876',
'MT1K904826'
)
)MTT
WHERE 1=1
AND MTT.REGISTERNO = RG.REGISTERNO
