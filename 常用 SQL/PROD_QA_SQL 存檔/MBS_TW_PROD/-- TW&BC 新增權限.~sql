SELECT * FROM SCSEMPDATA WHERE EMPNO IN ('2971') order by empno; --人員清單
SELECT * FROM SCSEMPTODEPT WHERE  EMPNO IN ('2971')  order by empno; --群組
SELECT * FROM SCSEMPTOOBJ WHERE EMPNO IN ('2971') order by empno; --人員權限
SELECT * FROM SCSSUBOBJGROUP WHERE DEPTCODE = 'N7' AND SUBGROUPCODE = 'M_ML_MASTER'
SELECT * FROM SCSDEPT WHERE DEPTCODE='52'
SELECT * FROM SCSDEPT D WHERE D.DEPTABBNAME LIKE '%客服一線%'
-- TBS BC 權限 MBS 也要同步加
-- 同步加人員地點
-- 離職人員
UPDATE DBS_MAJ.SCSEMPDATA ED SET ED.PASSWORD = '2971' , ED.DISABLED = 'N' , ED.QUITDATE = NULL WHERE ED.EMPNO IN ('9183') ;


