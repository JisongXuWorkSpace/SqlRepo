SELECT * FROM SCSEMPTODEPT WHERE  DEPTCODE IN ('LR3')
SELECT * FROM SCSEMPTOOBJ WHERE SUBGROUPCODE= 'LBS_USER'

SELECT * FROM SOMCONTACTADDRESS SCA WHERE SCA.ADDRESS_ID IS NULL
SELECT * FROM SCSEMPDATA WHERE EMPNAME='資湘竹'

-- LBS USER
SELECT COMPANYCODE , DEPTCODE , EMPNO , 'LBS_USER' ,  TO_CHAR( SYSDATE,'MM/DD/YYYY HH24:MI:SS') SYS_DATE , '' , 'Y'||TO_CHAR(SYSDATE,'YYMMDD') AS NOTE FROM SCSEMPTODEPT WHERE EMPNO IN ('2971') 
UNION
SELECT COMPANYCODE , DEPTCODE , EMPNO , 'BC_PM' ,  TO_CHAR( SYSDATE,'MM/DD/YYYY HH24:MI:SS') SYS_DATE , '' , 'Y'||TO_CHAR(SYSDATE,'YYMMDD') AS NOTE FROM SCSEMPTODEPT WHERE EMPNO IN ('2971')

ORDER BY 3


SELECT COMPANYCODE , DEPTCODE , EMPNO , 'LBS_USER' ,  TO_CHAR( SYSDATE,'MM/DD/YYYY HH24:MI:SS') SYS_DATE , '' , 'Y220527' FROM SCSEMPTODEPT WHERE EMPNO IN ('9183')

