warrantyCode
SELECT * FROM FIX_MT MT WHERE SLIP_NO = 'R200929038'

SELECT FA.WARRANTY_DATE ,FA.WARRANTY_CODE , FA.* FROM FIX_ARTICLE FA  WHERE SLIP_NO = 'R200929038';

SELECT * FROM FIX_SETTING WHERE KEY='WARRANTY_CODE'

 SELECT SER.SERVICETYPECODE, SER.SERVICETYPEDESC, PROP.DEFALUT_FLAG 
   FROM RPM_PROPID_SERVICE_TYPE PROP, RPMSERVICETYPECODE SER 
 WHERE 1 = 1 
    AND PROP.SERVICE_TYPE_CODE = SER.SERVICETYPECODE 
    AND PROP.DELETE_FLAG = 'N' 
    AND PROP.PROP_ID='3'
    
    
    SELECT * FROM RPM_PROPID_SERVICE_TYPE ST 
    
    SELECT * FROM RPM_PROPRIETOR
    
    SELECT * FROM   RPMSERVICETYPECODE
